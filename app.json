{
  "expo": {
    "name": "city-Map",
    "slug": "city-Map",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.kevinproanoricardobonilla.citymap",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBPk5xkI98kwT0ovYJjmHbo_5-8MrNHiEM"
        }
      },
      "versionCode": 1
    },
    "description": ""
  }
}